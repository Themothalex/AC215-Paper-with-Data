{
    "dataset_information": [
        {
            "formal_name": "Historical data from six German villages",
            "source_repository": "John Knodel (1988)",
            "timeframe": "1766-1863",
            "geographical_scope": "Six German villages",
            "population_sample": "3,418 mothers",
            "unit_of_analysis": "Individual-level data",
            "data_format": "Not specified",
            "sample_size": "15,077 childbirth events",
            "missing_data": "Not specified",
            "data_collection_method": "Recordkeeping in the six villages",
            "data_cleaning": "Not specified",
            "usage_in_paper": {
                "variable_type": "Binary and categorical",
                "specifics": "Used to assess impact of nutritional stress on maternal mortality"
            }
        }
    ],
    "study_context": {
        "study_title": "Effects of Nutritional Stress and Socio-Economic Status on Maternal Mortality in Six German Villages, 1766\u20131863",
        "authors": [
            {
                "name": "Francesco Scalone",
                "affiliations": [
                    "University of Bologna"
                ]
            }
        ],
        "publication_year": "Not specified",
        "research_domain": "Maternal mortality, historical demography"
    },
    "variables": {
        "dependent_variables": [
            {
                "name": "Maternal mortality",
                "type": "Binary",
                "measurement": "Risk of death within 42 days after childbirth"
            }
        ],
        "independent_variables": [
            {
                "name": "Socio-economic status",
                "type": "Categorical",
                "categories": [
                    "Higher and lower managers, professionals, clerical, and sales workers",
                    "Foremen and skilled workers",
                    "Farmers",
                    "Lower skilled workers",
                    "Unskilled workers",
                    "No occupation"
                ]
            },
            {
                "name": "Rural status",
                "type": "Binary",
                "categories": [
                    "Farm workers",
                    "Non-farm workers"
                ]
            },
            {
                "name": "Fluctuation in rye prices",
                "type": "Continuous",
                "categories": [
                    "Rye price (t)",
                    "Rye price (t-1)"
                ]
            }
        ],
        "control_variables": [
            {
                "name": "Age of mother",
                "type": "Continuous",
                "measurement": "Age at childbirth"
            },
            {
                "name": "Parity",
                "type": "Categorical",
                "measurement": "Number of previous births"
            },
            {
                "name": "Multiple births",
                "type": "Binary",
                "measurement": "Presence of twins or higher number of foetuses"
            }
        ]
    }
}